class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int m=matrix.length;
        
        for(int i=0;i<m;i++)// loop from rows
        {
            int n=matrix[i].length;
            for(int j=0;j<n;j++) // taking rows
            {
                if(matrix[i][j]==target){
                    return true;
                }
            }
        }
        return false;
    }
}
