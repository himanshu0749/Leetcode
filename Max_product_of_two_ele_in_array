int max=0;
        int n=nums.length;
        for(int i=0;i<n-1;i++){
            for(int j=i+1;j<n;j++){
                //if(n>0 && nums[i]>0){
                    if(max<=(nums[i]-1)*(nums[j]-1)) max=(nums[i]-1)*(nums[j]-1);
                //}
                
            }
        }
        return max;
