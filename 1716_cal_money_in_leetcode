class Solution {
public:
    int totalMoney(int n) {
        int i,j,a=0,count=0;
        for(i=0;i<n;i++){
            for(j=i+1;j<=7+i;j++){
                a+=j;
                count++;
                if(count==n){
                    return a;
                }
            }
        }
        
       return 0; 
    }
